    "jsonresume-theme-actual": "^0.1.6",
    "jsonresume-theme-onepage": "0.0.3",
    "jsonresume-theme-orbit": "^1.1.0",
    "jsonresume-theme-short": "^0.1.7",
    "resume-cli": "^3.0.6"
